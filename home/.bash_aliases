# Some tips:
#
# Type the following to list all currently active aliases:
#
#     $ alias
#
# Type the following to learn whether a particular command is an alias,
# function, etc:
#
#     $ type NAME
#

alias ack="/opt/local/bin/ack-5.12"

# Automatically call "workon" if .venv exists.
alias cd="venv_cd"

# The Keychain Access utility that comes with Mac OS X contains a
# useful dialog box for generating secure passwords.
alias keychain="open -a \"Keychain Access\""

# The -S option causes PATH to be searched.
alias mdown='perl -S Markdown.pl'
